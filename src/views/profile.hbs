    </div>
  </header>
  
  <main class="main-container">
    <div class="row">
      <div class="col-12">
        <h2>Create New Gime</h2>
        <form name="profile-form" method="post" action="/membersonly/gifts" accept-charset="utf-8">
          <label for="name">Category</label>
          <select name="category" required />
          <option value=""></option>
          <option value="artsandcrafts">Arts and Crafts</option>
          <option value="auto">Auto</option>
          <option value="beauty">Beauty</option>
          <option value="books">Books</option>
          <option value="clothing">Clothing</option>
          <option value="electronics">Electronics</option>
          <option value="home">Home</option>
          <option value="media">Media</option>
          <option value="kids">Kids and Toys</option>
          <option value="sports">Sports</option> 
          </select>
          <br><br>
          <input type="text" name="picture" value="" placeholder="Type Image url">
          <br />
          <input type="text" name="itemname" value="" placeholder="Item Name" required />
          <br />
          <input type="text" name="description" value="" placeholder="Description" />
          <br />
          $ <input type="number" name="price" placeholder="Estimated Price">
          <br />
          <button>Create</button>
        </form> 
        {{#each gift}}
      <div class="image">
        <img class="gift-image" src="{{this.imgUrl}}"> 
        <!-- {{this.userId}}<br /> -->
